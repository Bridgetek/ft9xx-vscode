    "version": "0.2.0",
    "configurations": [
        {
            "name": "FT90x_Debug (Flash Memory)",
            "type": "cppdbg",
            "MIMode": "gdb",
            "request": "launch",
            "environment": [
            ],
            "cwd": "${workspaceFolder}", 
            "program": "FT90x_Debug\\${workspaceFolderBasename}.elf",
            "preLaunchTask": "Build Project (FT90x_Debug)",
            "stopAtEntry": true,
            "miDebuggerPath": "${env:FT9XX_COMPILER}\\bin\\ft32-elf-gdb.exe",
            "miDebuggerArgs": "-x ${env:FT9XX_TOOLCHAIN}\\hardware\\.gdbinit",
            "miDebuggerServerAddress": "localhost:9998",
            "launchCompleteCommand": "None",
            "debugServerPath": "${env:FT9XX_TOOLCHAIN}\\programmer\\FT9xxProg.exe",
            "debugServerArgs": 
                "--loadflash FT90x_Debug\\${workspaceFolderBasename}.bin --onewire --device 0 --quiet --gdb",
            "serverStarted": "Waiting for GDB connection request..."
            //"rtos": "FreeRTOS",
        },
